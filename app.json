{
  "expo": {
    "name": "turbidity-app2",
    "slug": "turbidity-app2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AI_Generated_Img.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/AI_Generated_Img.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-build-properties"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.sofiene275.turbidityapp2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3d6b6cc1-a762-4b27-87c9-426c9aa10624"
      }
    }
  }
}
